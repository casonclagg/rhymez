{
  "name": "rhymez",
  "description": "Fast rhymes for javascript.",
  "version": "1.0.0",
  "main": "./lib/rhymez.js",
  "devDependencies": {
    "babel-cli": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib && cp ./src/data/* ./lib/data/",
    "test-es5": "npm run build && babel test --out-dir test-es5 && sed -i '.bak' 's/\\/src\\//\\/lib\\//' test-es5/*.js",
    "test": "npm run build && mocha --ui tdd --timeout 10000 --compilers js:babel-core/register --require test/mocha-babel",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [],
  "author": {
    "name": "Cason Clagg",
    "url": "https://github.com/casonclagg"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/casonclagg/rhymez"
  },
  "dependencies": {
    "co": "^4.6.0",
    "fs-extra": "^1.0.0",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.2",
    "node-dir": "^0.1.16"
  }
}
